MAKE_ANALYZER (M4A);

/*! Binary files only
 */
//--------------------------------------------------------------------
MAKE_IS_A (M4A)
//--------------------------------------------------------------------
{
  if (aGI.m_pBuffer->IsText ())
    return false;

  return aGI.m_pBuffer->GetInt32 (0) == 0x20000000 &&
         aGI.m_pBuffer->GetInt32 (4) == 0x70797466;
}

//--------------------------------------------------------------------
MAKE_DISPLAY (M4A)
//--------------------------------------------------------------------
{
  out_append (rc (HRC_GENLIB_M4A_HEADER));
}
